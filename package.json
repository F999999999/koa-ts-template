{
  "name": "koa-ts-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_ENV=development&&ts-node-dev -r tsconfig-paths/register main.ts",
    "prod": "set NODE_ENV=production&&ts-node -r tsconfig-paths/register main.ts",
    "test": "set NODE_ENV=test&&ts-node-dev -r tsconfig-paths/register main.ts",
    "pm2": "set NODE_ENV=production&&pm2 start main.ts",
    "devmodel": "set NODE_ENV=development&&ts-node-dev -r tsconfig-paths/register -r dotenv/config src/utils/sequelizeAuto.ts",
    "prodmodel": "set NODE_ENV=production&&ts-node-dev -r tsconfig-paths/register -r dotenv/config  src/utils/sequelizeAuto.ts",
    "testmodel": "set NODE_ENV=test&&ts-node-dev -r tsconfig-paths/register -r dotenv/config  src/utils/sequelizeAuto.ts",
    "devinitdb": "set NODE_ENV=development&&ts-node-dev -r tsconfig-paths/register -r dotenv/config src/utils/initDb.ts",
    "prodinitdb": "set NODE_ENV=production&&ts-node-dev -r tsconfig-paths/register -r dotenv/config  src/utils/initDb.ts",
    "testinitdb": "set NODE_ENV=test&&ts-node-dev -r tsconfig-paths/register -r dotenv/config  src/utils/initDb.ts",
    "build": "set NODE_ENV=production&&tsc"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.4",
    "koa-logger": "^3.2.1",
    "koa-onerror": "^4.2.0",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "log4js": "^6.9.1",
    "mysql2": "^3.6.0",
    "require-directory": "^2.1.1",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/koa": "^2.13.8",
    "@types/koa-json": "^2.0.20",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa2-cors": "^2.0.2",
    "@types/node": "^18.16.0",
    "@types/require-directory": "^2.1.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize-auto": "^0.8.8",
    "sequelize-typescript": "^2.1.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
